<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
     <th scope="col">Name</th>
     <th scope="col">email</th>
     <th scope="col">subject</th>
     <th scope="col">message</th>
    </tr>
  </thead>
  <tbody>
    {{#each msg}}
    <tr>
      <th scope="row">1</th>
      <td>{{this.name}}</td>
      <td>{{this.email}}</td>
      <td>{{this.text}}</td>
      <td>{{this.msg}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>